{
  "defaults": {
    "loose": true,
    "optional": [
      "es7.classProperties"
    ]
  },
  "builds": [
    {
      "dir": "lib",
      "version": "4.1.0.27",
      "options": {
        "blacklist": [
          "es6.forOf",
          "es6.templateLiterals",
          "es6.constants"
        ]
      }
    },
    {
      "dir": "lib-es5",
      "options": {
        "optional": [
          "runtime"
        ]
      }
    }
  ]
}
